{
    "gitbook": ">=3.2.0",
    "variables": {
      "py_version": "3.9",
      "py_release": "3.9.9",
      "py_min_version": "3.6",
      "py_min_release": "3.6.8",
      "pa_py_version": "3.8",
      "django_version": "3.2.13",
      "drf_version": "3.13.1",
      "django_filter_version": "22.1"
    },
    "pdf": {
        "fontSize": 16
    },
    "plugins": [
        "heading-anchors@1.0.3",
        "ga@1.0.1",
        "richquotes@0.0.9",
        "github@2.0.0",
        "language-picker",
        "codeblock-label",
        "sectionx-ex",
        "collapsible-menu"
    ],
    "pluginsConfig": {
        "ga": {
            "token": "UA-51680040-3"
        },
        "richquotes": {
            "default": false
        },
        "github": {
            "url": "https://github.com/learningequality/django-tutorial"
        },
        "language-picker": {
            "grid-columns": 3
        },
        "sectionx": {
            "tag": "b"
        }
